		"WTFPL"
	],
	"licensesDenied": [],
	"scripts": {
		"====[ Build ]====": "",
		"dev": "node server.js",
		"start": "NODE_ENV=production node server.js",
		"build": "npm run clean && npx next build",
		"lint": "npx next lint",
		"patch": "npm --no-git-tag-version version patch",
		"minor": "npm --no-git-tag-version version minor",
		"major": "npm --no-git-tag-version version major",
		"====[ Misc ]====": "",
		"reinstall": "npm run purge && npm i",
		"update": "npx npm-check-updates -u && npm run reinstall",
		"clean": "rm -rf .next/",
		"purge": "rm -rf .next/ node_modules/ package-lock.json"
	},
	"dependencies": {
		"next": "^14.2.3"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/lodash": "^4.17.5",
		"@types/node": "^20.12.12",
		"@types/react": "18.3.2",
		"@typescript-eslint/eslint-plugin": "^7.9.0",
		"@typescript-eslint/parser": "^7.9.0",
		"discord.js": "^14.15.2",
		"dotenv": "^16.4.5",
		"eslint": "^8.57.0",
		"eslint-config-next": "^14.2.3",
		"eslint-plugin-security": "^3.0.0",
		"json5": "^2.2.3",
		"lodash": "^4.17.21",
		"moment": "^2.30.1",
		"moment-timezone": "^0.5.45",
		"openai": "^4.47.1"
	}
}
